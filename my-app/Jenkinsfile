pipeline {
    agent {
      label 'linux'
    }
    tools {
        jdk 'oracle-7u80'
    }
    stages {
    stage('maven install') {
      steps {
        withMaven(globalMavenSettingsConfig: 'null', jdk: 'oracle-7u80', maven: 'Maven3', mavenSettingsConfig: 'null') {
        sh 'mvn clean install'
}
      }
    }

  }

  
}
